<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sarahahmad.musicalsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NowPlayingActivity"
            android:label="@string/NowPlayingActivity" />

        <activity
            android:name=".DetailsActivity"
            android:label="@string/DetailsActivity" />
        <activity
            android:name=".PaymentActivity"
            android:label="@string/PaymentActivity" />
        <activity
            android:name=".PlayingListsActivity"
            android:label="@string/PlayingListsActivity" />
        <activity
            android:name=".UploadActivity"
            android:label="@string/UploadActivity" />
    </application>

</manifest>